@import "p2pu-css-framework/lib/bootstrap";
// Bootstrap Responsive
@import "p2pu-css-framework/lib/responsive";
// Mixins already created for P2PU
@import "p2pu-css-framework/p2pu/p2pu-mixins";

// Import new fonts
@font-face {
  font-family: '3dumbregular';
  src: url('fonts/3dumb-webfont.eot');
  src: url('fonts/3dumb-webfont.eot?#iefix') format('embedded-opentype'), url('fonts/3dumb-webfont.woff') format('woff'), url('fonts/3dumb-webfont.ttf') format('truetype'), url('fonts/3dumb-webfont.svg#3dumbregular') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: '2dumbregular';
  src: url('fonts/2dumb-webfont.eot');
  src: url('fonts/2dumb-webfont.eot?#iefix') format('embedded-opentype'), url('fonts/2dumb-webfont.woff') format('woff'), url('fonts/2dumb-webfont.ttf') format('truetype'), url('fonts/2dumb-webfont.svg#2dumbregular') format('svg');
  font-weight: normal;
  font-style: normal;
}

@import url(http://fonts.googleapis.com/css?family=Kite+One);
@import url(http://fonts.googleapis.com/css?family=Arvo:400,700,400italic,700italic);

@mixin p2pu-btn($startColor, $endColor, $border) {
  color: $slate;
  @include gradient-vertical($startColor: $startColor, $endColor: $endColor);
  border-color: $startColor $endColor $border;
  border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
  filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
  cursor: pointer !important;
  font-size: 20px;
  padding: 7px 20px;
  margin-top: 1px;
  @include box-shadow(0 2px 2px rgba(0, 0, 0, 0.35));
  @include border-radius(5px);
  border-radius: 5px;
  font-weight: bold;
  &:hover {
    text-decoration: none;
    @include box-shadow(0 1px 2px rgba(0, 0, 0, 0.35));
  }
}
body{
  /*background-image: url('../img/paper.png');
  background-position: top left;
  background-repeat: repeat;*/
}
h1, h2,h3, h4, h5{
  font-family: 'Arvo', serif;
}
.p2pu-btn {
  @include p2pu-btn($startColor: $snow, $endColor: $snow, $border: $silver);
  //font-weight: bold;
  &:hover {
    color: $slate;
  }
}

.p2pu-btn-cyan {
  @include p2pu-btn($startColor: $cyan, $endColor: $cyan, $border: $cyan);
  color: $white;
  &:hover {
    color: $white;
  }
}

.p2pu-btn-apricot {
  @include p2pu-btn($startColor: $apricot, $endColor: $apricot, $border: $apricot);
  color: $white;
  &:hover {
    color: $white;
  }
}
.p2pu-btn-clemintine {
  @include p2pu-btn($startColor: $clemintine, $endColor: $clemintine, $border: $clemintine);
  color: $white;
  &:hover {
    color: $white;
  }
}
.p2pu-btn-olive {
  @include p2pu-btn($startColor: $olive, $endColor: $olive, $border: $olive);
  color: $white;
  &:hover {
    color: $white;
  }
}
.p2pu-btn-silver {
  @include p2pu-btn($startColor: $silver, $endColor: $silver, $border: $silver);
  color: $slate;
  &:hover {
    color: $slate;
  }
}
.text-appricot {
  color: $apricot;
}

.text-cyan {
  color: $cyan;
}

.navbar {
  .brand {
    display: block;
    width: 80px;
    height: 24px;
    background: url('../img/logo-p2pu-menu.png') no-repeat;
    padding: 0;
    margin: 17px 0;
    text-indent: -9999px;
    overflow: hidden;
  }

  .nav{
    margin: 9px 10px 0 20px;
    font-family: 'Arvo', sans-serif;
    li{
      a{
        text-shadow: none !important;
        font-size: 1.1em;

        &:hover, &:focus{
          color: #777;
          background-color: transparent;
        }

        &.p2pu-tab{
          font-size: 1.6em;
          border-left: 1px solid $silver;
        }
      }
    }
  }
}

.p2pu-panel-wrap {
  background-color: $white;
  display: none;
  .panel-contents {
    @extend .container;
    .us{
      float: left;
      margin-top: 20px;
    }
    .connect{
      @include makeColumn($columns: 4);
      float: right;
      li{
        line-height: 16px;
      }
      hr{
        margin: 10px 0;
      }
      a{
        font-size: 1.2em;
        &:hover{
          text-decoration: none;
        }
      }
    }
    .supporters{
      margin-right: 20px;
      @include makeColumn($columns: 6);
      float: right;
      h4{
        text-align: center;
      }
      a{
        display: block;
        text-align: center;
        &.view-all{
          margin: 20px 0;
        }
      }

    }
  }
}

.p2pu-color-divider-wrap {
  border-bottom: solid 6px $apricot;
}

.p2pu-color-divider-wrap::before, .p2pu-color-divider-wrap::after {
  content: "";
  height: 6px;
  width: 25%;
  position: absolute;
  left: 25%;
  background: $olive;
}

.p2pu-color-divider::before, .p2pu-color-divider::after {
  content: "";
  height: 6px;
  width: 25%;
  position: absolute;
  left: 50%;
  background: $cyan;
}

.p2pu-color-divider::after {
  left: 75%;
  background: $clemintine;
}

/********/
.jumbotron {
  margin-top: 65px;
  margin-bottom: 50px;
  text-align: center;
  position: relative;
  background-image: url('../img/photo-learn_one.jpg');
  background-size: cover;
  background-position: 50% 55%;
  text-align: center;
  height: 334px;
  margin-bottom: -60px;
  font-size: 21px;
  line-height: 1.3em;
  text-shadow: 0 0 12px rgba(0,0,0,0.5),0 0 32px rgba(0,0,0,0.25),0 1px rgba(0,0,0,0.75);
  .main-feature{
    color: $white;
  }
  .lead {
    color: $white;
    padding-bottom: 22px;
    font-size: 21px;
    font-weight: 200;
    text-transform: uppercase;
    line-height: 50px;
    margin-left: 15px;
    font-family: $baseFontFamily;
  }

  .do-something{
    height: 60px;
    text-align: left;
    .p2pu-tour-btn {
      padding: 17px 25px;
      &:hover {
        text-decoration: none;
      }
    }
  }
}

.actions {
  @extend .clearfix;
  margin: 0 0 50px 0;
  background-color: #f3f4f8;
  position: relative;
  @include border-radius(15px);
  li {
    @include makeColumn($columns: 4);
    margin-left: 5%;
    margin-top: 30px;
    a {
      display: block;
      font-family: 'Kite One', sans-serif;
      &:hover {
        text-decoration: none;
        h2 {
          color: $apricot;
        }
      }
    }
    img {
      float: left;
      margin-right: 20px;
    }
    p {
      overflow: hidden;
    }
  }
}
.whole{
  padding: 40px 0;
  background-image: url('../img/mochaGrunge.png');
  background-position: left top;
  width: 100%;
  position: absolute;
  left: 0;
  .container{
    position: relative;
    font-size: 60px;
    color: white;
    text-align: center;
  }
  .headline-icon{
    font-size: 3em;
    position: absolute;
    right: 0;
    top: -80px;
    text-shadow: 1px 2px 3px $white;
    color: $pewter;
  }
  &.appricot{
    border-top: 3px solid $apricot;
    border-bottom: 3px solid $apricot;
  }
  &.cyan{
    border-top: 3px solid $cyan;
    border-bottom: 3px solid $cyan;
  }
  &.olive{
    border-top: 3px solid $olive;
    border-bottom: 3px solid $olive;
  }
  &.clemintine{
    border-top: 3px solid $clemintine;
    border-bottom: 3px solid $clemintine;
  }
  &.silver{
    border-top: 3px solid $silver;
    border-bottom: 3px solid $silver;
  }
  &.white{
    border-top: 3px solid $white;
    border-bottom: 3px solid $white;
  }
  &.purple{
    border-top: 3px solid $purple;
    border-bottom: 3px solid $purple;
  }
}
.billboard {
  padding: 40px 20px;
  margin-bottom: 40px;
  @include box-shadow(0 0 0 1px #fff inset);
  background: $white;
  border-bottom: 1px solid #ddd;
  zoom: 1;
  margin-top: 230px;
  @extend .clearfix;
  .img-large {
    @extend .img-polaroid;
    float: left;
    margin-right: 20px;
    width: 165px;
    margin-top: 20px;
  }
  .controls {
    a {

      @extend .pull-right;
      margin: 20px 10px;
      i {
        margin-left: 20px;
      }
    }
  }
  .thumbnail {
    min-height: 445px;
    position: relative;
    .thumbnail-controls {
      margin-top: 20px;
      padding: 20px 0;
      position: absolute;
      bottom: 20px;
    }
  }
  .we-are{
    list-style-type: none;
    margin-left: 0;
    @extend .clearfix;
    li{
      margin: 20px;
      position: relative;
      text-align: center;
      background: #fff;
      padding: 10px;
      width: 128px;
      height: 160px;
      border: 1px solid #aaa;
      float: left;
      cursor: pointer;
      -webkit-border-radius: 4px;
      -moz-border-radius: 4px;
      -ms-border-radius: 4px;
      -o-border-radius: 4px;
      border-radius: 4px;
      -webkit-box-shadow: 0 1px 4px rgba(0,0,0,0.2);
      -moz-box-shadow: 0 1px 4px rgba(0,0,0,0.2);
      box-shadow: 0 1px 4px rgba(0,0,0,0.2);
      &:first-child{
        margin-left: 0;
      }
      img{
        width: 128px;
        height: 128px;
      }
      .caption{
        padding-top: 20px;
        text-align: left;
      }
    }
  }
  .badges-carousel{
    padding: 0px;
    position: relative;
    height: 450px;
    list-style: none;
    margin-left: -20px;
    li{
      position: absolute;
      opacity: 1;
      z-index: 280;
      left: 10%;
      top:20%;
      z-index: 8;
      opacity: 0.6;
      &:hover{
        opacity: 1;
      }
    }
    li + li{
      left: 25%;
      top:10%;
      z-index: 9;
      opacity: 0.8;
      &+li{
        left:40%;
        top:0;
        z-index: 10;
        opacity: 1;
        &+li{
          left:55%;
          top:10%;
          z-index: 9;
          opacity: 0.8;
          &+li{
          left:70%;
          top:20%;
          z-index: 8;
          opacity: 0.6;
        }
        }
      }
    }

  }
  .blog-list{
    li{
      a{
        font-weight: bold;
        h3{
          color: $cyan;
        }
        &:hover{
          text-decoration: none;
          h3{
            color: $apricot;
          }

        }
      }
      .icon-chevron-right{
        margin-left: 50px;
      }
    }
  }
  .schools{
    .thumbnail-controls {
      @extend .clearfix;
      margin-bottom: 30px;
      a{
        @extend .pull-right;
        padding-right: 30px;
      }

    }
    .school {
      border: 1px solid $silver;
      @include border-radius(10px);
      border-bottom: 1px solid #dbdcdf;
      padding: 30px 76px 36px 262px;
      position: relative;
      margin-bottom: 20px;
    }
    .school::before {
      content: '';
      position: absolute;
      border-top: 19px solid #dbdcdf;
      border-right: 19px solid transparent;
      border-left: 19px solid transparent;
      left: 90%;
      bottom: -19px;
      margin-left: -20px;
    }
    .school::after {
      content: '';
      position: absolute;
      border-top: 18px solid #fff;
      border-right: 18px solid transparent;
      border-left: 18px solid transparent;
      left: 90%;
      bottom: -18px;
      margin-left: -19px;
    }
    .school img {
      position: absolute;
      top: 50px;
      left: 60px;
      width: 70px;
    }
  }
  .moocs{
    .icon-cogs{
      font-size: 20em;
      float: left;
      margin-right: 20px;
    }
  }
  .description{
    overflow: hidden;
  }
}
.timeline{
  .text-align-center{
    text-align: center;
    position: relative;
    z-index: 10;
  }
}
.timeline-events {
  margin: 0 auto;
  padding: 0;
  width: 7px;
  height: 760px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  background: #848689 url(../img/timeline-bg.jpg) no-repeat bottom center;
  position: relative;
  margin-top: 50px;
}

.timeline-events li {
  background: $white;
  width: 20px;
  height: 20px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  border-radius: 20px;
  border: 5px solid #848689;
  list-style: none;
  margin-bottom: 107px;
  margin-left: -12px;
  position: relative;
  .popover{
    opacity: 1;
    background: #e8ebef;
    color: #848689;
    padding: 15px 0 15px 0;
    font-size: 18px;
    -webkit-border-radius: 8px;
    -moz-border-radius: 8px;
    border-radius: 8px;
    display: block;
    top: -20px;
    .arrow{
      top:20%;
    }
    &.top{
      top: -150px;
      left: -113px;
      .arrow{
        top:auto;
      }
    }
    &.left{
      left: -250px;
    }
    &.right{
      left: 30px;
    }
  }
}

.power-users{
  .thumbnail{
    background-color: $white;
    padding-left: 20px;
  }
  .power-user-img{
    margin-right: 30px;
    width: 120px;
    height: 120px;
    margin-top: 30px;
  }
  .caption{
    overflow: hidden;
  }
  .user-courses{
    li{
      float: left;
      margin-right: 30px;
      img{
        width: 100px;
        height: 100px;
      }
    }
  }
}:
